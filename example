function test(a,b){
	y = 0;
	z = 1;
	x = y + z + a;
	return x + b;
}

x = 1;
y = 1.2;
t = x + y;

print(x,y,t);

if(t <= 3){
    print(t);
    x = x + 1;
    x = test(3, x);
}
print(t, x);

function fib(x){
    if(x <= 2){
        return 1;
    }
    return fib(x-1) + fib(x-2);
}

x = fib(10);
print(x);